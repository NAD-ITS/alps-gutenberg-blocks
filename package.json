    "@wordpress/i18n": "^3.14.0",
    "@wordpress/url": "^2.17.0",
    "classnames": "^2.2.6",
    "gettext-parser": "^4.0.4",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@octokit/rest": "^18.0.3",
    "archiver": "^5.0.0",
    "cgb-scripts": "^1.23.1",
    "chalk": "^4.1.0",
    "fs-extra": "^9.0.1",
    "luxon": "^1.25.0",
